
@model List<mvcIdentity.Areas.SuperAdmin.Model.PermissionViewModel>


@{
    ViewBag.Title = "Index";
}

<div>
    <table>
        <tr>
            <th>
                controller
            </th>
            <th>
                action
            </th>
            <th>
                description
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Controller)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Action)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
            </tr>
        }
    </table>
</div>

